#PASSWORD GENERATOR (8 CHARACTERS)

import random

upper = ['A', 'B', 'C', 'D', 'E']
lower = ['a','b','c','d','e']
numb = ['1','2','3','4','5']
special = ['!','@','#','$','%']

passwrd = random.choice(upper) + random.choice(lower) + random.choice(numb) + random.choice(special) + random.choice(upper) + random.choice(lower) + random.choice(numb) + random.choice(special)

print(passwrd)
